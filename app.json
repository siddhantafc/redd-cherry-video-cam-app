{
  "expo": {
    "name": "Red Cherry Live Video Cam",
    "slug": "redcherry",
    "icon": "./app/assets/images/icon-fc-512.webp",
    "version": "1.211.1025_beta",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#d9a25b",
      "image": "./app/assets/images/friends-club-splash.png"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera for video calls",
        "NSMicrophoneUsageDescription": "This app needs access to microphone for voice and video calls"
      },
      "bundleIdentifier": "com.redcherry.redcherrylivevideocam"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "icon": "./app/assets/images/icon-fc-512.webp",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/icon-fc-512.webp",
        "backgroundColor": "#410237"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "com.redcherry.redcherrylivevideocam"
    }
  }
}
